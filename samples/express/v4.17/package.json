{
  "name": "typescript-starter-express-swagger",
  "version": "4.17.1",
  "description": "TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "tsc -w -p tsconfig.build.json",
    "start:dev": "ts-node-dev --respawn -- src/main.ts",
    "start:debug": "ts-node-dev --respawn --debug -- src/main.ts",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --testRegex=\\.e2e-spec.ts",
    "test:e2e:watch": "jest --watch --testRegex=\\.e2e-spec.ts"
  },
  "dependencies": {
    "@femike/swagger-protect": "^2.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cookie-parser": "^1.4.6",
    "express": "4.17.1",
    "rimraf": "3.0.2",
    "swagger-ui-express": "^4.5.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/common": "^8.4.7",
    "@nestjs/core": "^8.4.7",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.2.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "4.17.13",
    "@types/node": "ts4.3",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "27.5.1",
    "prettier": "2.7.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "^7.1.0",
    "supertest": "^6.2.4",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "ts-node-paths": "^1.0.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "4.3.5"
  },
  "jest": {
    "preset": "ts-jest",
    "verbose": true,
    "coverageDirectory": "coverage",
    "testRegex": "\\.spec\\.ts"
  }
}