{
  "name": "typescript-starter-express-swagger",
  "version": "1.0.0",
  "description": "TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "tsc -w -p tsconfig.build.json",
    "start:dev": "ts-node-dev --respawn -- src/main.ts",
    "start:debug": "ts-node-dev --respawn --debug -- src/main.ts",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --testRegex=\\.e2e-spec.ts",
    "test:e2e:watch": "jest --watch --testRegex=\\.e2e-spec.ts"
  },
  "dependencies": {
    "@femike/swagger-protect": "^9.0.0",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.2",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "rimraf": "3.0.2",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.1.6",
    "@types/node": "ts4.7",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.2",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.7.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "ts-node-paths": "^1.0.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "preset": "ts-jest",
    "verbose": true,
    "coverageDirectory": "coverage",
    "testRegex": "\\.e2e-spec\\.ts"
  }
}
