{
  "name": "typescript-starter-fastify-swagger",
  "version": "1.0.0",
  "description": "Typescript starter fastify express",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "tsc -w -p tsconfig.build.json",
    "start:dev": "ts-node-dev --respawn -- src/main.ts",
    "start:debug": "ts-node-dev --respawn --debug -- src/main.ts",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest -template=e2e-spec.ts"
  },
  "author": "femike <femike@mail.ru>",
  "license": "MIT",
  "dependencies": {
    "@femike/swagger-protect": "^2.0.0",
    "@femike/swagger-protect-ui": "^2.0.0",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "fastify-cookie": "^5.3.1",
    "fastify-static": "^4.2.3",
    "fastify-swagger": "^4.8.4",
    "rimraf": "3.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/common": "^8.0.5",
    "@nestjs/core": "8.0.5",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "5.0.9",
    "@types/cookie-parser": "^1.4.2",
    "@types/node": "14.17.7",
    "@types/supertest": "2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "jest": "27.0.6",
    "prettier": "2.3.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.3.0",
    "supertest": "6.1.4",
    "ts-jest": "27.0.4",
    "ts-loader": "9.2.5",
    "ts-node": "10.1.0",
    "ts-node-dev": "^1.1.8",
    "ts-node-paths": "^1.0.1",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  }
}
